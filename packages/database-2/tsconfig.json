{
  "extends": "@remoola/typescript-config/nestjs.json",
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": true,
    "outDir": "./dist",
    "baseUrl": "."
  },
  "include": [
    "./src/**/*.ts",
  ],
  "exclude": [
    "node_modules",
    "./prisma.config.ts"
  ]
}
