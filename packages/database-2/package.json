{
  "name": "@remoola/database-2",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -b -v",
    "clean": "rm -rf ./node_modules ./dist ./generated/**",
