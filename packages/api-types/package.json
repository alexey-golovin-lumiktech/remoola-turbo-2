{
  "name": "@remoola/api-types",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "yarn build --watch",
    "build": "tsc -b -v",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "clean": "rm -rf ./node_modules ./dist"
  },
  "main": "./dist/entry.js",
  "types": "./dist/entry.d.ts",
  "files": [
    "./dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/entry.js",
      "require": "./dist/entry.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/mapped-types": "*",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@remoola/eslint-config": "^0.0.0",
    "@remoola/typescript-config": "^0.0.0",
    "@types/node": "^22.10.7",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "typescript": "5.8.2"
  }
}
