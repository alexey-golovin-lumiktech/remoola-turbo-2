{
  "extends": "@remoola/typescript-config/nestjs.json",
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": true
  },
  "include": [
    "**/*.ts",
    "prisma.config.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
