{
  "name": "@wirebill/backend",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "workspaces": {
    "packages": [
      "libs/**/*"
    ]
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "db:make-migration": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:make",
    "db:migrate": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:latest",
    "db:migrate:up": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:up",
    "db:migrate:list": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:list",
    "db:rollback": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:rollback",
    "db:seed": "cross-env NODE_ENV=development ./node_modules/.bin/knex seed:run",
    "db:seed:add_default_admins": "cross-env NODE_ENV=development ./node_modules/.bin/knex  seed:run --specific=20230329083231_add_default_admins.ts",
    "db:seed:add_dummy_consumers": "cross-env NODE_ENV=development ./node_modules/.bin/knex  seed:run --specific=20230329083233_add_dummy_consumers.ts",
    "db:seed:add_dummy_payment_requests": "cross-env NODE_ENV=development ./node_modules/.bin/knex  seed:run --specific=20230329083243_add_dummy_payment_requests.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "knex:init": "./node_modules/.bin/knex init -x ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "**/*.{ts,json}": []
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@wirebill/shared-admin(|/.*)$": "<rootDir>/libs/shared-admin/src/$1",
      "^@wirebill/shared-consumer(|/.*)$": "<rootDir>/libs/shared-consumer/src/$1"
    },
    "rootDir": ".",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.2.1",
    "@wirebill/pdf-generator-package": "^0.0.947247134",
    "@wirebill/shared-common": "^0.0.947247134",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "google-auth-library": "^8.7.0",
    "husky": "^8.0.3",
    "joi": "^17.9.1",
    "knex": "^2.4.2",
    "lint-staged": "^13.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nestjs-knex": "^2.0.0",
    "nestjs-stripe": "^1.0.0",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.1",
    "puppeteer": "^20.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "stripe": "^12.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/json5": "^2.2.0",
    "@types/knex": "^0.16.1",
    "@types/lodash": "^4.14.192",
    "@types/moment": "^2.13.0",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/passport": "^1.0.12",
    "@types/passport-http": "^0.3.9",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/puppeteer": "^7.0.4",
    "@types/request": "^2.48.8",
    "@types/stripe": "^8.0.417",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "28.1.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "ttypescript": "^1.5.15",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=16.16",
    "npm": ">=8.11"
  }
}
