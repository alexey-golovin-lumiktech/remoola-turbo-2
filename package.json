{
  "name": "@wirebill/backend",
  "version": "0.0.1",
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "db:make-migration": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:make",
    "db:migrate:down": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:down",
    "db:migrate:latest": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:latest",
    "db:migrate:list": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:list",
    "db:migrate:rollback": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:rollback",
    "db:migrate:up": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:up",
    "db:seed": "cross-env NODE_ENV=development ./node_modules/.bin/knex seed:run",
    "db:seed:add_default_admins": "cross-env NODE_ENV=development ./node_modules/.bin/knex  seed:run --specific=20230329083231_add_default_admins.ts",
    "db:seed:add_dummy_consumers": "cross-env NODE_ENV=development ./node_modules/.bin/knex  seed:run --specific=20230329083233_add_dummy_consumers.ts",
    "db:seed:add_dummy_contacts": "cross-env NODE_ENV=development ./node_modules/.bin/knex  seed:run --specific=20230329083343_add_dummy_contacts.ts",
    "db:seed:add_dummy_exchange_rates": "cross-env NODE_ENV=development ./node_modules/.bin/knex  seed:run --specific=20230329083443_add_dummy_exchange_rates.ts",
    "db:seed:add_dummy_payment_methods": "cross-env NODE_ENV=development ./node_modules/.bin/knex  seed:run --specific=20230329083543_add_dummy_payment_methods.ts",
    "db:seed:add_dummy_payment_requests": "cross-env NODE_ENV=development ./node_modules/.bin/knex  seed:run --specific=20230329083243_add_dummy_payment_requests.ts",
    "format": "prettier --write \"./{apps,libs,src}/**/*.{ts,js}\" ",
    "knex:init": "./node_modules/.bin/knex init -x ts",
    "lint": "dotenv -v NODE_OPTIONS=--max-old-space-size=4096 -v DEBUG=eslint:cli-engine eslint \"./{apps,libs,src}/**/*.{ts,js}\" --fix",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:prod": "node dist/main",
    "start:vercel:dev": "cross-env NODE_ENV=development IS_ON_VERCEL=true nest start --watch",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{ts}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "roots": [
      "<rootDir>/src/"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.567.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@wirebill/pdf-generator-package": "^0.0.1276147011",
    "@wirebill/shared-common": "^0.0.1276147011",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "express-session": "^1.18.0",
    "google-auth-library": "^9.9.0",
    "husky": "^9.0.11",
    "joi": "^17.13.1",
    "knex": "^3.1.0",
    "lint-staged": "^15.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nestjs-knex": "^2.0.0",
    "nestjs-stripe": "^1.0.0",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.5",
    "puppeteer": "^22.7.1",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "stripe": "^15.4.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.11.5",
    "@types/request": "^2.48.12",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "ttypescript": "^1.5.15",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=20.11.1",
    "npm": ">=10.2.4"
  }
}