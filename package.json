    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^11.0.0",
    "axios": "^1.10.0",
    "@wirebill/pdf-generator-package": "^0.0.1322481904",
    "@wirebill/shared-common": "^0.0.1322481904",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "express-session": "^1.18.0",
    "google-auth-library": "^9.9.0",
    "googleapis": "^137.1.0",
    "husky": "^9.0.11",
    "joi": "^17.13.1",
    "knex": "^3.1.0",
    "lint-staged": "^15.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nestjs-knex": "^2.0.0",
