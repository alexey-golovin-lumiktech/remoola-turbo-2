{
  "name": "@wirebill/backend",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": "",
  "workspaces": {
    "packages": [
      "libs/**/*"
    ]
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "db:make-migration": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:make",
    "db:migrate:down": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:down",
    "db:migrate:latest": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:latest",
    "db:migrate:list": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:list",
    "db:migrate:rollback": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:rollback",
    "db:migrate:up": "cross-env NODE_ENV=development ./node_modules/.bin/knex migrate:up",
    "db:seed": "cross-env NODE_ENV=development ./node_modules/.bin/knex seed:run",
    "db:seed:add_default_admins": "cross-env NODE_ENV=development ./node_modules/.bin/knex  seed:run --specific=20230329083231_add_default_admins.ts",
    "db:seed:add_dummy_consumers": "cross-env NODE_ENV=development ./node_modules/.bin/knex  seed:run --specific=20230329083233_add_dummy_consumers.ts",
    "db:seed:add_dummy_contacts": "cross-env NODE_ENV=development ./node_modules/.bin/knex  seed:run --specific=20230329083343_add_dummy_contacts.ts",
    "db:seed:add_dummy_payment_requests": "cross-env NODE_ENV=development ./node_modules/.bin/knex  seed:run --specific=20230329083243_add_dummy_payment_requests.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "knex:init": "./node_modules/.bin/knex init -x ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "**/*.{ts,json}": []
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^@wirebill/shared-admin(|/.*)$": "<rootDir>/libs/shared-admin/src/$1",
      "^@wirebill/shared-consumer(|/.*)$": "<rootDir>/libs/shared-consumer/src/$1"
    },
    "rootDir": ".",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.405.0",
    "@aws-sdk/lib-storage": "^3.405.0",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.0.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/swagger": "^7.1.10",
    "@wirebill/pdf-generator-package": "^0.0.991334230",
    "@wirebill/shared-common": "^0.0.991334230",
    "addresser": "^1.1.20",
    "addressit": "^1.8.2",
    "aws-sdk": "^2.1450.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "google-auth-library": "^9.0.0",
    "husky": "^8.0.3",
    "joi": "^17.10.1",
    "knex": "^2.5.1",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nestjs-knex": "^2.0.0",
    "nestjs-stripe": "^1.0.0",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "puppeteer": "^21.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.2.0",
    "stripe": "^13.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.17",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.4",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.197",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.9",
    "@types/nodemailer": "^6.4.9",
    "@types/passport": "^1.0.12",
    "@types/passport-http": "^0.3.9",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/pg": "^8.10.2",
    "@types/request": "^2.48.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "ttypescript": "^1.5.15",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=16.16",
    "npm": ">=8.11"
  }
}
