{
  "name": "@wirebill/shared-consumer",
  "version": "0.0.1",
  "license": "MIT",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "node": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "start:dev": "tsup --watch"
  },
  "devDependencies": {
    "@types/node": "^20.3.3",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "tsup": {
    "dts": true,
    "clean": true,
    "splitting": false,
    "format": [
      "cjs"
    ],
    "target": "esnext",
    "entry": [
      "src/**/index.ts"
    ]
  }
}
