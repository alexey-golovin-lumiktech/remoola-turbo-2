{
  "name": "@remoola/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn migrate:up && nest build",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "clean": "rimraf node_modules",
    "dev:envs": "dotenv -e ./.env.development -e ./.env --",
    "start:dev": "yarn dev:envs nest start --watch",
    "dev": "yarn start:dev",
    "migrate:up": "yarn dev:envs typeorm-ts-node-commonjs migration:run -d ./src/config/orm.config.ts",
    "migrate:up2": "yarn build && yarn dev:envs typeorm migration:run -- -d ./dist/src/config/orm.config.js"
  },
  "dependencies": {
    "@remoola/api-types": "^0.0.0",
    "@aws-sdk/client-s3": "^3.872.0",
    "@aws-sdk/s3-request-presigner": "^3.872.0",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.58.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "ioredis": "^5.7.0",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.0.17",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^10.0.0",
    "@remoola/env": "^0.0.0"
  },
  "peerDependencies": {
    "typeorm": "*"
  },
  "resolutions": {
    "typeorm": "*"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@remoola/eslint-config": "^0.0.0",
    "@remoola/jest-config": "^0.0.0",
    "@remoola/typescript-config": "^0.0.0",
    "@types/express": "^4.17.17",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4",
    "@types/cookie-parser": "^1.4.9",
    "@types/node": "^24.3.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2"
  }
}
