{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "builder": "tsc"
  },
  "generateOptions": {
    "spec": false,
    "flat": true
  },
  "projects": {
    "pdf-generator-package": {
      "type": "library",
      "root": "./libs/pdf-generator-package",
      "entryFile": "index",
      "sourceRoot": "./libs/pdf-generator-package/src",
      "compilerOptions": {
        "tsConfigPath": "./libs/pdf-generator-package/tsconfig.lib.json"
      }
    },
    "shared-admin": {
      "type": "library",
      "root": "./libs/shared-admin",
      "entryFile": "index",
      "sourceRoot": "./libs/shared-admin/src",
      "compilerOptions": {
        "tsConfigPath": "./libs/shared-admin/tsconfig.lib.json"
      }
    },
    "shared-consumer": {
      "type": "library",
      "root": "./libs/shared-consumer",
      "entryFile": "index",
      "sourceRoot": "./libs/shared-consumer/src",
      "compilerOptions": {
        "tsConfigPath": "./libs/shared-consumer/tsconfig.lib.json"
      }
    },
    "shared-common": {
      "type": "library",
      "root": "./libs/shared-common",
      "entryFile": "index",
      "sourceRoot": "./libs/shared-common/src",
      "compilerOptions": {
        "tsConfigPath": "./libs/shared-common/tsconfig.lib.json"
      }
    }
  }
}
