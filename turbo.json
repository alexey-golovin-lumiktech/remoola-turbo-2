{
  "$schema": "https://turborepo.com/schema.json",
  "globalEnv": [
    "NODE_ENV",
    "NEXT_PUBLIC_API_BASE_URL",
    "DATABASE_URL",
    "REDIS_URL",
    "OPENAI_API_KEY",
    "JWT_EXPIRES_IN",
    "JWT_REFRESH_TTL",
    "JWT_ACCESS_TTL",
    "JWT_SECRET",
    "JWT_REFRESH_SECRET",
    "FORCE_PAYMENT_RESULT",
    "S3_BUCKET",
    "AWS_REGION",
    "S3_PUBLIC_BASE",
    "COOKIE_DOMAIN",
    "POSTGRES_TIMEZONE",
    "POSTGRES_HOST",
    "POSTGRES_PORT",
    "POSTGRES_USER",
    "POSTGRES_PASSWORD",
    "POSTGRES_DB",
    "POSTGRES_SSL",
    "COOKIE_SECURE",
    "SECURE_SESSION_SECRET",
    "PORT",
    "VERCEL",
    "VERCEL_URL",
    "JWT_ACCESS_SECRET",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "NEST_APP_EXTERNAL_ORIGIN",
    "ADMIN_EMAIL"
  ],
  "ui": "stream",
  "globalDependencies": [
    "package.json",
    "yarn.lock",
    ".env",
    ".env.*"
  ],
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ],
      "env": [
        "DATABASE_URL"
      ]
    },
    "lint": {},
    "test": {},
    "clean": {
      "cache": false
    },
    "test:e2e": {},
    "deploy": {
      "dependsOn": [
        "build"
      ],
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:migrate": {
      "cache": false,
      "persistent": true
    },
    "db:deploy": {
      "cache": false
    }
  }
}
